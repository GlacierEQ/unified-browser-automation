{
  "name": "unified-browser-automation",
  "version": "2.0.0",
  "description": "Apex unified browser automation system with Selenium 4.34, Playwright 1.56, Puppeteer, MCP integration, and AI orchestration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:playwright": "playwright test",
    "test:selenium": "jest --testMatch='**/selenium/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/integration/**/*.test.ts'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen",
    "docker:build": "docker build -t unified-browser-automation .",
    "docker:run": "docker run -p 8787:8787 unified-browser-automation",
    "mcp:start": "node dist/mcp/server.js"
  },
  "dependencies": {
    "@playwright/test": "^1.56.0",
    "playwright": "^1.56.0",
    "selenium-webdriver": "^4.34.0",
    "puppeteer": "^23.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "playwright-extra": "^4.3.6",
    "express": "^4.21.1",
    "fastify": "^5.2.0",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.1",
    "winston": "^3.17.0",
    "pino": "^9.5.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "redis": "^4.7.0",
    "ioredis": "^5.4.2",
    "bull": "^4.16.3",
    "@prisma/client": "^6.2.1",
    "googleapis": "^144.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "uuid": "^11.0.3",
    "nanoid": "^5.0.9",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "boxen": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.14",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "@playwright/browser-chromium": "^1.56.0",
    "@playwright/browser-firefox": "^1.56.0",
    "@playwright/browser-webkit": "^1.56.0"
  },
  "keywords": [
    "browser-automation",
    "selenium",
    "playwright",
    "puppeteer",
    "web-automation",
    "mcp",
    "ai-orchestration",
    "testing",
    "scraping",
    "e2e-testing"
  ],
  "author": "GlacierEQ <GLACIER.EQUILIBRIUM@GMAIL.COM>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GlacierEQ/unified-browser-automation.git"
  }
}